{
  "name": "basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --opts ./test/mocha.opts",
    "hello": "node_modules/.bin/ts-node src/hello.ts",
    "coverage": "nyc mocha",
    "format": "prettier --config ./.prettierrc.js --write \"{src, test}/**/*.ts\"",
    "eslint": "eslint --cache --color \"{src, test}/**/*.ts\""
  },
  "nyc": {
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
